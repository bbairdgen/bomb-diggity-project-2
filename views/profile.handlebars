<div class="row">
  <div class="col-auto">
    <h2>Welcome, {{username}}!</h2>
  </div>
</div>

<div class="row">
  <div class="col-md-6">
    <div class="row">
        <div class="col s12 m6">
          <div class="card blue-grey darken-1">
            <div class="card-content white-text">
              <span class="card-title">Submit A Correction:</span>
            </div>
          </div>
        </div>
      </div>

    <form class="form new-autocorrect-form">
      <div class="form-group">
        <label for="typo">Typo:</label>
        <input class="form-input" type="text" id="typo" name="typo" />
      </div>
      <div class="form-group">
        <label for="usage">Times Used:</label>
        <input class="form-input" type="number" id="usage" name="usage" />
      </div>
      <div class="form-group">
        <label for="correction">Correction:</label>
        <textarea class="form-input" id="correction" name="correction"></textarea>
      </div>
      <div class="form-group">
        <button type="submit" class="btn btn-primary blue-grey darken-1">Create</button>
      </div>
    </form>
  </div>

  <div class="row">
    <div class="col-md-6">
        <button type="submit" class="btn btn-primary show-cor blue-grey darken-1">Show All Corrections</button>
    </div>
</div>

<script src="./js/profile.js"></script>