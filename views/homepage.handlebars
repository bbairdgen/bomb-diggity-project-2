<div>hello</div>

<div class="row mt-4">
  {{#if autocorrects.length}}
    <div class="col-md-6 correction-list">
      <h3>all autocorrects</h3>
      {{#each autocorrects as |autocorrect|}}

        <div class="row mb-2">
          <div class="col-md-8">
            <h4><a
                href="/api/autocorrects/{{autocorrect.id}}"
              >{{autocorrect.typo}}|{{autocorrect.correction}}
              </a>
            </h4>

          </div>
        </div>
      {{/each}}
    </div>
  {{/if}}
</div>